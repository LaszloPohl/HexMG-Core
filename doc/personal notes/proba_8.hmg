HEXMG 1.0

.FUNCTION diodeeq P=2
10 _MULC RET P0 1e13
20 _ADDC RET RET 1
30 _LN RET RET
40 _MULC RET RET 0.026
50 _MUL RET RET P1
.END FUNCTION diodeeq

.MODEL circuit SUBCIRCUIT N=4 V=1
I1 I N0 GND V0
R1 R N0 N1 10
FCI1 FCI IN=1 F=diodeeq(IN0 P0) N1 N3 N2 1e5
VI1 VI N3 GND N2 0 0 0 0 1e5
.END MODEL circuit

.CREATE circ1 circuit GND=R0

.PROBE P1 V circ1 N0 N1 N2 N3

.SET circ1.V0 0.01

.RUN circ1 DC
.SAVE FILE="proba_8.res" P1

.END
