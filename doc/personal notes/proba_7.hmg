HEXMG 1.0

.FUNCTION square P=1 V=2 CT=1
4  _CONST V1 0
5  _ILOAD V0 CT0.X V1
10 _MUL V0 V0 V0
15 _ADD V0 V0 0
20 _MUL RET V0 P0
.END FUNCTION square

.FUNCTION sq2 P=1 V=0 CT=1
10 square CT0 RET P0
.END FUNCTION sq2

.MODEL circuit SUBCIRCUIT N=2
R1 R N0 GND 10
I1 I N0 GND 0.25
I2 FCI IN=0 CIN=0 P=1 CT=1 F=square(CT0 P1) R1 N1 GND 0.5 0.1
.END MODEL circuit

.CREATE circ1 circuit GND=R0

.PROBE P1 V circ1 N0 N1

.RUN circ1 DC
.SAVE FILE="probe_3.res" P1

.END
